  "bugs": {
    "url": "https://github.com/comatosino/translator/issues"
  },
  "homepage": "https://github.com/comatosino/translator#readme",
  "dependencies": {
    "axios": "^0.26.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@mui/icons-material": "^5.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
