    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.23",
    "@types/uuid": "^8.3.4",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  }
