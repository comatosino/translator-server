  },
  "homepage": "https://github.com/comatosino/translator#readme",
  "dependencies": {
    "@mui/icons-material": "^5.5.0",
    "@types/mongoose": "^5.11.97",
    "axios": "^0.26.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.7",
    "uuid": "^8.3.2",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.0.0",
    "eslint": "^8.10.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.5.0",
    "typescript": "^4.6.2"
  }
}
