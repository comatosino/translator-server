{
  "name": "translator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "prebuild": "tsconfig.json --fix",
    "build": "tsc --project .",
    "build-prod": "cd client && npm install && npm run build && cd .. && npm install && npm run build",
    "server": "nodemon --ignore client/* --exec ts-node server.ts",
    "client": "cd client && npm start && cd ..",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install": "cd client && npm install",
    "seed": "cd db && ts-node seed.ts",
    "postinstall": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/comatosino/translator.git"
  },
